<Application x:Class="CoffeeStore.StockManagement.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CoffeeStore.StockManagement.App"
             xmlns:converter="clr-namespace:CoffeeStore.StockManagement.App.Converter"
             StartupUri="View/CoffeeOverviewView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:PriceConverter x:Key="localPriceConverter"></converter:PriceConverter>
            <local:ViewModelLocator x:Key="mainViewModelLocator"></local:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
